{
  "extends": "@robino/tsconfig/bundler.json",
  "include": [
    "./app/**/*",
    "**/.server/**/*",
    "**/.client/**/*",
    ".react-router/types/**/*"
  ],
  "compilerOptions": {
    "paths": {
      "~/*": ["./app/*"]
    },
    "rootDirs": [".", "./.react-router/types"],
    "types": ["node", "vite/client"],
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "noImplicitAny": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false
  }
}
